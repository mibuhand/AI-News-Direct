{
    "base_url": "https://huggingface.co",

    "schemas": {

        "organizations": {
            "url": "/organizations",

            "org_list":[
                "/openai",
                "/meta-llama",
                "/anthropic",
                "/xai-org",
                "/mistralai",
                "/ibm-granite",
                "/deepmind",
                "/google",
                "/facebook",
                "/microsoft",
                "/amazon",
                "/nvidia",
                "/apple",

                "/stabilityai",
                "/black-forest-labs",
                "/Kijai",
                "/Comfy-Org",

                "/Qwen",
                "/deepseek-ai",
                "/tencent",
                "/baidu",
                "/moonshotai",
                "/THUDM",
                "/ByteDance",
                "/ByteDance-Seed",
                "/bytedance-research",
                "/MiniMaxAI",
                "/openbmb",
                "/BAAI",

                "/HuggingFaceTB",
                "/huggingface",
                "/unsloth",

                "/kyutai",

                "/pollen-robotics",
                "/lerobot",
                "/agibot-world"

            ],

            "pages": [
                "/activity/models",
                "/activity/datasets",
                "/activity/papers"
            ],

            "schema":     {
                "name": "activities",
                "baseSelector": "div.org-profile-content div.mb-7",
                "fields": [
                    {
                        "name": "author",
                        "selector": ".mb-3 .text-smd .inline-block .contents .whitespace-nowrap.underline",
                        "type": "attribute",
                        "attribute": "href"
                    },

                    {
                        "name": "action",
                        "selector": ".mb-3 .text-smd .flex.items-baseline span span",
                        "type": "text"
                    },

                    {
                        "name": "target",
                        "selector": ".mb-3 .text-smd .flex.items-baseline span",
                        "type": "text"
                    },

                    {
                        "name": "date",
                        "selector": ".ml-4",
                        "type": "text"
                    },

                    {
                        "name": "objects",
                        "selector": "div.space-y-3 article.overview-card-wrapper",
                        "type": "nested_list",
                        "fields": [
                            {
                                "name": "obj_url",
                                "selector": "a",
                                "type": "attribute",
                                "attribute": "href"
                            },

                            {
                                "name": "obj_title",
                                "selector": "h4",
                                "type": "text"
                            },
                                    
                            {
                                "name": "obj_info",
                                "selector": "div.mr-1",
                                "type": "text"
                            }
                        ]
                    }
                ]
            }
        },

        "models": {
            "url": "/models"
        },

        "papers": {
            "url": "/papers/date"
        }
    }
}